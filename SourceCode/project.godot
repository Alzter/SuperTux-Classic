; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "AudioLoader",
"language": "GDScript",
"path": "res://autoload/WAVAudioImporter.gd"
}, {
"base": "Node",
"class": "StateMachine",
"language": "GDScript",
"path": "res://scenes/state_machine.gd"
} ]
_global_script_class_icons={
"AudioLoader": "",
"StateMachine": ""
}

[application]

config/name="SuperTux Classic"
config/description="A ground-up remaster of SuperTux Milestone 1 in the Godot Engine!"
run/main_scene="res://scenes/menus/TitleScreen.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="SuperTuxClassic"
config/icon="res://icon.png"
config/macos_native_icon="res://icon.icns"
config/windows_native_icon="res://icon.ico"

[autoload]

Global="*res://autoload/Global.gd"
Scoreboard="*res://autoload/Scoreboard.tscn"
Music="*res://autoload/Music.tscn"
ParticleCache="*res://autoload/ParticleCache.tscn"
SaveManager="*res://autoload/SaveManager.gd"
ResolutionManager="*res://autoload/ResolutionManager.gd"
WorldmapManager="*res://autoload/WorldmapManager.gd"
MobileControls="*res://autoload/MobileControls.tscn"
UserLevels="*res://autoload/UserLevelsManager.gd"
WAVAudioImporter="*res://autoload/WAVAudioImporter.gd"

[display]

window/size/width=1280
window/size/height=720

